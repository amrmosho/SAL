<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pcland15.ismail.sal.panel">


    <ScrollView
        android:fillViewport="true"
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="5dp">

        <LinearLayout
            style="@style/main_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navbar"
            android:layout_marginTop="50dp"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/signs"
                android:textAppearance="?android:attr/textAppearanceLarge"


                />

            <LinearLayout
                style="@style/textarea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <Button
                    android:id="@+id/button10"
                    style="@style/button"
                    android:hint="@string/panel_cat"

                    android:onClick="goto_mysignCats" />

                <Button
                    android:id="@+id/button9"
                    style="@style/button"
                    android:layout_gravity="center_horizontal"
                    android:hint="@string/panel_sign"
                    android:onClick="goto_sign"


                    />
            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/books"
                android:textAppearance="?android:attr/textAppearanceLarge"


                />

            <LinearLayout
                style="@style/textarea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <Button
                    style="@style/button"
                    android:hint="@string/panel_cat"

                    android:onClick="goto_booksCats" />

                <Button
                    style="@style/button"
                    android:layout_gravity="center_horizontal"
                    android:hint="@string/add_book"
                    android:onClick="goto_book" />
            </LinearLayout>

            <TextView
                android:id="@+id/textView51"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/panel_quiz"
                android:textAppearance="?android:attr/textAppearanceLarge"


                />

            <LinearLayout
                style="@style/textarea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <Button
                    android:id="@+id/button13"
                    style="@style/button"
                    android:hint="@string/panel_cat"

                    android:onClick="goto_quiz_cat"

                    />

                <Button
                    android:id="@+id/button12"
                    style="@style/button"
                    android:layout_gravity="center_horizontal"
                    android:hint="@string/addquiz_q"
                    android:onClick="goto_quiz_Q" />

                <Button
                    android:id="@+id/button13d"
                    style="@style/button"
                    android:layout_gravity="center_horizontal"
                    android:hint="@string/addquiz_a"
                    android:onClick="goto_quiz_A" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView513"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/users"
                android:textAppearance="?android:attr/textAppearanceLarge"


                />

            <LinearLayout
                style="@style/textarea"
                android:layout_width="match_parent"
                android:orientation="vertical"


                >

                <Button
                    android:id="@+id/button133"
                    style="@style/button"
                    android:hint="@string/panel_cat"

                    android:onClick="goto_user_Cats"

                    />

                <Button
                    android:id="@+id/button123"
                    style="@style/button"
                    android:layout_gravity="center_horizontal"

                    android:hint="@string/add_user"
                    android:onClick="goto_user" />


            </LinearLayout>


        </LinearLayout>
    </ScrollView>
        <RelativeLayout
            android:id="@+id/navbar33333"

            style="@style/navbar"
            android:layout_width="match_parent"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp">


            <ImageButton
                android:id="@+id/button4"
                style="@style/navbar_back_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="goback"
                android:text="@string/back" />

            <ImageButton
                android:id="@+id/button5"
                style="@style/navbar_send_button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:onClick="userRegSend"

                android:text="@string/send" />

            <TextView
                android:id="@+id/textView2"
                style="@style/navbar_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/newcat_title"
                android:textAppearance="?android:attr/textAppearanceLarge"

                />

        </RelativeLayout>





</RelativeLayout>
