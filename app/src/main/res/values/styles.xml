<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowIsFloating">false</item>


    </style>





    <style name="signtheme" parent="AppTheme">

        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/sapp_background</item>
        <item name="android:textColor">#fff</item>
    </style>









    <style name="home_comment_text"
>
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">5dp</item>



    </style>


    <style name="homeBT">
        <item name="android:background">@null</item>
        <item name="android:scaleType">center</item>
        <item name="android:layout_width">118dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textColor">#ffba01</item>
    </style>


    <style name="home_main_bt">
        <item name="android:background">@null</item>

        <item name="android:layout_height">100dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>

        <item name="android:layout_width">match_parent</item>









    </style>

    <style name="hometTitle">
        <item name="android:textColor">#052b39</item>
        <item name="android:layout_marginTop">15dp</item>

        <item name="android:textStyle">bold|italic</item>
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="android:textAlignment">center</item>
    </style>




    <style name="label">
        <item name="android:textColor">#fff</item>


    </style>

    <style name="textarea">
        <item name="android:background">@drawable/textarea_shape</item>
        <item name="android:textColor">#333</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>

    </style>

    <style name="Spinner">
        <item name="android:background">@drawable/button_shape</item>
        <item name="android:layout_height">50dp</item>


    </style>




    <style name="form_label" parent="label">


    </style>

    <style name="title" parent="label">
        <item name="android:layout_width">match_parent</item>


        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>


        <item name="android:textColor">@color/yallowButtonColor</item>


    </style>
    <style name="title_quiz" parent="title">

    </style>

    <style name="input_text">

        <item name="android:textAlignment">center</item>



    </style>
    <style name="input_login">

        <item name="android:background">@color/colorPrimary</item>
        <item name="android:textSize">12dp</item>
        <item name="android:ems">10</item>
        <item name="android:layout_height">33dp</item>



    </style>



    <style name="form_input_text" parent="input_text">


        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>

    </style>

    <!-- navBarStyle-->
    <style name="navbar">
        <item name="android:background">@drawable/nav_bar_bg</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>

        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_alignParentTop">true</item>

xx


    </style>

    <style name="navbarbottom">
        <item name="android:background">@drawable/nav_bar_bg</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">match_parent</item>

        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>


        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_marginLeft">0dp</item>
    </style>
    <style name="navbar_back_button">

        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>



        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>

        <item name="android:background">@drawable/back_bt_bg</item>

        <item name="android:textColor">@null</item>
<item name="android:textSize">1dp</item>
        <item name="android:shadowColor">@null</item>


    </style>

    <style name="button">
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/button_shape</item>
        <item name="android:textColor">@color/textColorPrimary</item>

        <item name="android:layout_height">50dp</item>

    </style>



    <style name="image_button"  parent="button" >
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@drawable/image_shape</item>
        <item name="android:src">@drawable/image_icone</item>
    </style>
    <style name="video_button" parent="image_button"   >

        <item name="android:src">@drawable/video_icone</item>
    </style>




    <style name="navbar_send_button">

        <item name="android:layout_gravity">right</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:background">@color/yallowButton</item>
        <item name="android:textColor">@color/yallowButtonColor</item>


    </style>

    <style name="navbar_title">
        <item name="android:textColor">@color/textColorPrimary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <!-- navBarStyle-->



    <style name="cat_list_title">
        <item name="android:textColor">@color/yallowButtonColor</item>
        <item name="android:textAlignment">center</item>

    </style>

    <style name="items_list_title">
        <item name="android:textColor">@color/yallowButtonColor</item>

        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">15dp</item>

    </style>


    <style name="items_list_image">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart">true</item>

        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginLeft">15dp</item>


    </style>
    <!-- Cat_list-->
    <!-- Cat_list-->
    <style name="items_list_area">
        <item name="android:background">@drawable/image_shape</item>

    </style>
    <style name="cat_list_area">
        <item name="android:background">@drawable/cat_item_bg</item>
        <item name="android:radius">5dp</item>

    </style>


    <style name="cat_list_image">
        <item name="android:layout_marginTop">10dp</item>

        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:radius">15dp</item>

    </style>
    <!-- Cat_list-->


    <style name="searchBT">

        <item name="android:background">@drawable/seearcg_bg</item>
        <item name="android:layout_width">300dp</item>


        <item name="android:layout_centerHorizontal">true</item>

        <item name="android:layout_height">50dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:textAlignment">center</item>


    </style>



    <style name="main_data">

        <item name="android:padding">10dp</item>
        <item name="android:paddingTop">10dp</item>

        <item name="android:paddingRight">10dp</item>

        <item name="android:paddingLeft">10dp</item>

        <item name="android:paddingBottom">10dp</item>

        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>


    <style name="main_image">

        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:paddingBottom">40dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingRight">10dp</item>



    </style>
    <style name="table_row">
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>


    </style>

    <style name="form_table_row" parent="table_row"></style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="signtheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="signtheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
